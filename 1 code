#include "UART_STACK.h" 


 

int main(void)
{
   sei(); // enable global interrupt

/*******************************Initilization *******************************/
	DIO_Init();
	LCD_Init();
    UART_Init();
	UART_StackInit();
/****************************************************************************/
u8 timer=0;
     while(1)
    {
		// just timer to check the code
		LCD_GoTo_WriteString(0,0,"Stack");
		 LCD_GoTo_WriteNumber(1,13,timer);
		 timer++;
		 _delay_ms(400);
    }
}
 
 

 